import diagnosis.Patient


rule "Losartan"
    //salience 15
    dialect "java"
    when
    p1: Patient  (disease.equals("heart failure"), age != Age.CHILD,pregnant == false,
    !medicines.contains("Hypersensitivity to Losartan"),!medicines.contains("medicines with aliskiren"), !comorbidity.contains("diabetes"), !comorbidity.contains("kidney malfunction"),!comorbidity.contains("severe liver failure"))

then
    p1.setTreatments("losartan");
    System.out.println("Losartan is the best treatment for the patient");
end
